#!/usr/bin/env modbuild

pbuild::add_to_group 'Tools'

pbuild::set_download_url "https://sourceforge.net/projects/gnuplot/files/$P/$V/$P-$V.tar.gz"
pbuild::set_docfiles 'Copyright' 'ChangeLog' 'NEWS' 'README'

pbuild::make_all

